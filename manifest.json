{
  "name": "Hacker News UX",
  "version": "1.3.2",
   "manifest_version": 2,  
  "description": "Hacker News improve user experience experiment.",
  
  "web_accessible_resources": [
    "images/ajax-loader.gif",
    "style.css",
    "jquery-1.9.1.min.js",
    "jquery.min.map"
    
   ],
  "content_scripts": [
    {
      "matches": ["https://news.ycombinator.com/*", "http://news.ycombinator.net/*","http://news.ycombinator.org/*","http://news.ycombinator.com/*","http://hackerne.ws/*","https://hackerne.ws/*"],     
    	"css":["style.css"],
      "js": ["myscript.js","jquery-1.9.1.min.js"],
    	"run_at":"document_end"
    }
  ],
  "permissions": [
   "https://news.ycombinator.com/*","http://news.ycombinator.net/*","http://news.ycombinator.org/*", "http://news.ycombinator.com/*","http://hackerne.ws/*", "tabs",
   "storage"
  ]
}